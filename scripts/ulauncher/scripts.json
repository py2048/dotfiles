[
    {
        "name": "Restart plasma",
        "command": "plasmashell --replace &",
        "terminal": false
    },
    {
        "name": "Restart spoof-dpi",
        "command": "sudo -i systemctl restart spoof-dpi.service",
        "terminal": false
    },
    {
        "name": "Restart libvirtd",
        "command": "sudo -i systemctl restart --user libvirtd.service",
        "terminal": false
    },
    {
        "name": "Restart teamview",
        "command": "teamviewer --daemon restart",
        "terminal": false
    },
    {
        "name": "Restart kmonad",
        "command": "systemctl restart --user kbd.service",
        "terminal": false
    },
    {
        "name": "Reset WM-1000XM4",
        "command": "pactl set-card-profile bluez_card.14_3F_A6_6D_BC_65 off && pactl set-card-profile bluez_card.14_3F_A6_6D_BC_65 a2dp-sink-aac && pactl set-default-sink bluez_output.14_3F_A6_6D_BC_65.1",
        "terminal": false
    },
    {
        "name": "Unicode",
        "command": "kitty nvim -c LatexSymbols -c 'set buftype=nofile'",
        "terminal": false
    },
    {
        "name": "Restart logid",
        "command": "sudo -i systemctl restart --user logid.service",
        "terminal": false
    },
    {
        "name": "Share clipboard",
        "command": "/home/dquan/.local/bin/wh.sh",
        "terminal": false
    },
    {
        "name": "Restart network",
        "command": "sudo -A systemctl restart NetworkManager",
        "terminal": false
    }
]
